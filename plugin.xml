<?xml version="1.0" encoding="UTF-8"?>

<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0"
	xmlns:android="http://schemas.android.com/apk/res/android"
	id="admob-ionic-demo"
	version="1.0.0">
      
    <name>Cordova AdMob plugin for Ionic demo</name>
	<description>Demo app for Cordova AdMob plugin, using ionic framework</description>
	<author>Jaive Hall</author>
	<license>MIT</license>
	<keywords>rjfun,admob,google,ad</keywords>
    <repo>https://github.com/jaivehall/admob-ionic-demo.git</repo>
    <issue>https://github.com/jaivehall/admob-ionic-demo/issues</issue>

	<engines>
	    <engine name="cordova" version=">=3.0" />
	</engines>

	<dependency id="cordova-plugin-admobpro" />
    
	<dependency id="cordova-plugin-device" />
	<dependency id="cordova-plugin-console" />
    
	<dependency id="com.ionic.keyboard" />

    <platform name="android">
    </platform>
     
     <platform name="ios">
	</platform>
</plugin>
